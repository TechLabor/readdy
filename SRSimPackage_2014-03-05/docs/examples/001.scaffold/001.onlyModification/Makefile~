	BNG2.pl spass_w.bngl
	cp spass_w.gdat w_scf.gdat

#/home/stud/bid02/gerdl/RulesIn3D/ModelingSoftware/BioNetGen_2.0.44/Perl2/BNG2.pl spass_wo.bngl
