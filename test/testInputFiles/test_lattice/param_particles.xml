<?xml version="1.0" encoding="UTF-8"?>

<!--
    Document   : param.particles.xml
    Created on : 17. October 2011, 10:23
    Author     : Schoeneberg
    Description:
    
	space dimension is nm = 10^-9 m
	time dimension is s = s

	D = x mum^2 s^-1 = 10^+6 nm^2 s^-1
	
-->

<param_particles version="1.1">
	<particle>		
        <type>A</type>
		<diffusionConstant>1e+6</diffusionConstant>		
		<collisionRadiusMap>
			<collisionRadius>
				<!-- 'default' radius is taken for default except otherwise defined-->
				<partnerType>default</partnerType>
				<radius>1.4</radius>
			</collisionRadius>
			<collisionRadius>
				<partnerType>B</partnerType>
				<radius>1</radius>
			</collisionRadius>
		</collisionRadiusMap>		
		<reactionRadiusMap>
			<reactionRadius>
				<partnerType>default</partnerType>
				<radius>1</radius>
			</reactionRadius>
		</reactionRadiusMap>
	</particle>
	<particle>		
        <type>B</type>
		<diffusionConstant>0.5e+6</diffusionConstant>		
		<collisionRadiusMap>
			<collisionRadius>
				<partnerType>default</partnerType>
				<radius>1.185</radius>
			</collisionRadius>
			<collisionRadius>
				<partnerType>A</partnerType>
				<radius>5</radius>
			</collisionRadius>
		</collisionRadiusMap>		
		<reactionRadiusMap>
			<reactionRadius>
				<partnerType>default</partnerType>
				<radius>1</radius>
			</reactionRadius>
		</reactionRadiusMap>
	</particle>
</param_particles>
	