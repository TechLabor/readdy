<?xml version="1.0" encoding="UTF-8"?>

<!--
	space dimension is nm = 10^-9 m
	time dimension is s = s

	D = x mum^2 s^-1 = 10^+6 nm^2 s^-1
-->

<param_particles version="1.1">
	<particle>		
        <type>A</type>
		<diffusionConstant>1.5e+6</diffusionConstant>		
		<collisionRadiusMap>
			<collisionRadius>
				<partnerType>default</partnerType>
				<radius>2.0</radius>
			</collisionRadius>
		</collisionRadiusMap>		
		<reactionRadiusMap>
			<reactionRadius>
				<partnerType>default</partnerType>
				<radius>2.0</radius>
			</reactionRadius>
		</reactionRadiusMap>
	</particle>	
</param_particles>
